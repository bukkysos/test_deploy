{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "2015",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-trailing-spaces": ["error"],
        "consistent-return": "error",
        "no-useless-return": "error",
        "prettier/prettier": "error"
    }
}
